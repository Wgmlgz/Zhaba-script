{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Zhaba-script programming language",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.zh",
          "match": "\\b(?|@|for)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.zh",
      "begin": "'",
      "end": "'",
      "patterns": [
        {
          "name": "constant.character.escape.zh",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.zh"
}

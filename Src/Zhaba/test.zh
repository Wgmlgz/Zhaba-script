include 'std/std.zh'
include 'std/vecint.zh'
include 'std/range.zh'
include 'std/util.zh'

fn main
  auto vec = newVecInt()
  
  auto r = 0..10
  @ r.nxt()
    vec.push_back(rng() % 100)

  put 'before sort: '
  out &vec
  qsort(vec.begin() vec.end())
  put 'after sort:  '
  out &vec
  out vec.back()
  vec.dtor()

include 'std.zh'

type Vec
  int x y

impl Vec
  fn set int x int y
    self.x = x
    self.y = y
  
  fn int dist 
    <<< self.x + self.y

fn Vec newVec int x int y
  Vec v
  v.set(x y)
  <<< v

fn main
  int i = 0
  Vec vec
  vec.set(2, 3)
  @ i=0 i<10 i=i+1
    auto t = newVec(rng() % 10, rng() % 10)
    ? t.dist() < 5: out 1
    \ out 0

include 'std.zh'

type Vec
  int x y

fn Vec newVec
  Vec v
  v.x = 0
  <<< v

impl Vec
  fn int getX
    <<< self.x
  fn int getY
    <<< self.y
  fn setX int x
    self.x = x
  fn setY int y
    self.y = y

fn int vecGet VecP v
  <<< v.x

fn vecSet VecP v int val
  v.x = val

fn main
  int i = 0
  Vec vec
  @ (i = 0) (i < 10) (i = i + 1)
    out(rng() % 10)

  

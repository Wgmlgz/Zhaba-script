lop int + int a: <<< a;
op 5  bool %% int a int b: <<< !(a % b);
op 5  bool !% int a int b: <<< !(a %% b);

op 16 += u8R a u8 b: a = a + b
op 16 -= u8R a u8 b: a = a - b
op 16 /= u8R a u8 b: a = a / b
op 16 %= u8R a u8 b: a = a % b
op 16 *= u8R a u8 b: a = a * b


op 16 += u16R a u16 b: a = a + b
op 16 -= u16R a u16 b: a = a - b
op 16 /= u16R a u16 b: a = a / b
op 16 %= u16R a u16 b: a = a % b
op 16 *= u16R a u16 b: a = a * b


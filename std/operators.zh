lop ++ intR val: val += 1
lop -- intR val: val -= 1
lop ++ u8R val: val = val + 1u8
lop -- u8R val: val = val - 1u8

op 4 int ** int a int n
  res := 1
  @ n != 0
    ? n !% 2: res *= a
    a *= a
    n /= 2
  <<< res

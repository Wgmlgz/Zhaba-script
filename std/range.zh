/**
 * Range type for loops or something
 * example:
 * @ i 2..5: out i
 */

use operators

impl int
  fn next
    *slf = *slf + 1
  fn bool uneq int other
    <<< *slf != other
    
type Range: int begin end

impl Range
  fn int begin: <<< slf.begin
  fn int end: <<< slf.end

op 9 Range .. int begin int end
  Range slf
  slf.begin = begin
  slf.end = end
  <<< slf
  